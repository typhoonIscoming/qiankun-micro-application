        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-vue": "^6.2.2",
        "filemanager-webpack-plugin": "^3.0.0",
        "html-webpack-plugin": "^4.0.4",
        "husky": "^4.3.8",
        "less": "^3.9.0",
        "less-loader": "^7.2.1",
        "lint-staged": "^10.5.3",
        "node-sass": "^5.0.0",
        "postcss-pxtorem": "^5.1.1",
        "sass-loader": "^10.1.0",
